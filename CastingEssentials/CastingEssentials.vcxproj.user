<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="14.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|Win32'">
    <LocalDebuggerCommand>$(TF_GAME_DIR)\..\hl2.exe</LocalDebuggerCommand>
    <LocalDebuggerCommandArguments>-insecure -game tf -windowed -w 1600 -h 900 -novid +exec CastingEssentials +playdemo stv_test_demo_2 -high +exec launch_settings</LocalDebuggerCommandArguments>
    <LocalDebuggerWorkingDirectory>$(TF_GAME_DIR)\..\</LocalDebuggerWorkingDirectory>
    <DebuggerFlavor>WindowsLocalDebugger</DebuggerFlavor>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|Win32'">
    <LocalDebuggerCommand>$(TF_GAME_DIR)\..\hl2.exe</LocalDebuggerCommand>
    <LocalDebuggerCommandArguments>-insecure -game tf -windowed -w 1600 -h 900 -novid +exec CastingEssentials +playdemo stv_test_demo_2 -high +exec launch_settings</LocalDebuggerCommandArguments>
    <LocalDebuggerWorkingDirectory>$(TF_GAME_DIR)\..\</LocalDebuggerWorkingDirectory>
    <DebuggerFlavor>WindowsLocalDebugger</DebuggerFlavor>
  </PropertyGroup>
  <PropertyGroup>
    <ShowAllFiles>true</ShowAllFiles>
  </PropertyGroup>
</Project>